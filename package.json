{
  "name": "@superfaceai/service-client",
  "version": "4.0.0",
  "description": "Client to access Superface services. (If you are not from Superface Team, you probably don't need this library)",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:superfaceai/service-client.git",
  "author": "Superface Team",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.release.json --outDir dist",
    "clean": "rimraf dist/",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.config.js --runInBand",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "express": "^4.18.1",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  }
}
