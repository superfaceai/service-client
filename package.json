{
  "name": "@superfaceai/service-client",
  "version": "3.0.0",
  "description": "Client to access Superface services. (If you are not from Superface Team, you probably don't need this library)",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:superfaceai/service-client.git",
  "author": "Superface Team",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.release.json --outDir dist",
    "clean": "rimraf dist/",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.config.js --runInBand",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.0"
  }
}
